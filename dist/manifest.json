{
  "manifest_version": 3,
  "name": "Zen-Filter",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "http://localhost:5000/*",
    "<all_urls>"
  ]
}
